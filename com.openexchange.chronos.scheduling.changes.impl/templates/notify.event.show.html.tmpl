<div class="calendar-detail">
    <div class="date">
        <div class="interval">
            ${formatters.interval}
        </div>
        <div class="day">
            ${formatters.dateSpec}
        </div>
    </div>
    <div class="title clear-title">
        ${(event.summary!"")?html}
    </div>
    <div class="location">
        ${(event.location!"")?html}
    </div>
    <div class="calendar-buttons" style="display:none"></div>
    <div class="note">
        ${labels.noteAsHTML}
    </div>
    <div class="participants">
        <div class="label">
            ${labels.attendeesLabel}
        </div>
        <div class="attendee-list">
            <#list attendees as attendee>
            <div class="attendee">
                <span class="person">${attendee.cn!""?html}</span>
                <#if (attendee.partStat??)>
                    <#if (attendee.partStat == "ACCEPTED" )>
                    <span class="status accepted">&#10003</span>
                    <#elseif (attendee.partStat == "DECLINED" )>
                    <span class="status declined"> x</span>
                    <#elseif (attendee.partStat == "TENTATIVE" )>
                    <span class="status tentative"> ?</span>
                    </#if>
                </#if>
                <span class="comment">${(attendee.comment!"")?html}</span>
            </div>
            </#list>
        </div>
        <div class="attendee-clear"></div>
    </div>
    <div class="participants">
        <#if resources??>
            <div class="label">
                ${labels.resourcesLabel}
            </div>
            <div class="resource-list">
                <#list resources as resource>
                <div class="resource">
                    <span class="resource">${resource.cn?html}</span>
                </div>
                </#list>
            </div>
        </#if>
        <div class="resource-clear"></div>
    </div>
    <div>
        <div class="label">
            ${labels.detailsLabel}
        </div><span class="detail-label">${labels.showAsLabel}&nbsp;</span><span class="detail"><span class="shown_as_label ${labels.showAsClass}">${labels.showAs}</span></span><br>
        <span class="detail-label">${labels.createdLabel}&nbsp;</span><span class="detail"><span>${formatters.created}</span> <span>-</span> <span>${labels.creator}</span></span>
        <#if labels.directLink??>
            <br>
            <span class="detail-label">${labels.directLinkLabel}&nbsp;</span><span class="detail"><a href="${labels.directLink}">${labels.directLink}</a> <span></span>
        </#if>

    </div>
    <div class="attachmentNote">
        ${labels.attachmentNote!""}
    </div>
    <div class="justification">
        ${labels.justification!""}
    </div>
</div>