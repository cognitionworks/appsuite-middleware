# Specifies the configuration tree path to  JSlob path mapping
# <config-tree-path> + ">" + <JSlob-id> + "//" + <JSlob-path>
# my/config/tree/path>my/jslob//subpath

# Core LOB
language > io.ox/core//language
timezone > io.ox/core//timezone
serverVersion > io.ox/core//serverVersion
folder > io.ox/core//folder

identifier > identifier

# Timezones
availableTimeZones > io.ox/core/settingOptions//availableTimeZones

# Contact Collector
modules/mail/contactCollectOnMailAccess > io.ox/mail//contactCollectOnMailAccess
modules/mail/contactCollectOnMailTransport > io.ox/mail//contactCollectOnMailTransport

# Mail
modules/mail/defaultseparator > io.ox/mail//defaultseparator
modules/mail/defaultFolder > io.ox/mail//defaultFolder
modules/mail/addresses > io.ox/mail//addresses
modules/mail/defaultaddress > io.ox/mail//defaultaddress
modules/mail/sendaddress > io.ox/mail//defaultSendAddress
modules/mail/appendmailtext > io.ox/mail//appendMailTextOnReply
modules/mail/deletemail > io.ox/mail//removeDeletedPermanently
modules/mail/forwardmessage > io.ox/mail//forwardMessageAs
modules/mail/allowhtmlimages > io.ox/mail//allowHtmlImages
modules/mail/inlineattachments > io.ox/mail//allowHtmlMessages
modules/mail/emoticons > io.ox/mail//displayEmoticons
modules/mail/colorquoted > io.ox/mail//isColorQuoted
mail/folder > io.ox/mail//folder
mail/vcard > io.ox/mail//vcard
ui/mail/replyTo > io.ox/mail//showReplyTo
modules/mail/linewrap > io.ox/mail//lineWrapAfter
modules/mail/vcard > io.ox/mail//appendVcard
modules/mail/unifiedinboxidentifier > io.ox/mail//unifiedInboxIdentifier

# CalDAV
modules/caldav/active > io.ox/caldav//active
modules/caldav/url > io.ox/caldav//url

# Migration
gui > io.ox/migration//gui

# Calendar notifications
modules/calendar/notifyAcceptedDeclinedAsCreator > io.ox/calendar//notifyAcceptedDeclinedAsCreator
modules/calendar/notifyAcceptedDeclinedAsParticipant > io.ox/calendar//notifyAcceptedDeclinedAsParticipant
modules/calendar/notifyNewModifiedDeleted > io.ox/calendar//notifyNewModifiedDeleted

# Calendar FreeBusy
modules/calendar/calendar_freebusy > io.ox/calendar//freeBusy

# Task notifications
modules/tasks/notifyAcceptedDeclinedAsCreator > io.ox/tasks//notifyAcceptedDeclinedAsCreator
modules/tasks/notifyAcceptedDeclinedAsParticipant > io.ox/tasks//notifyAcceptedDeclinedAsParticipant
modules/tasks/notifyNewModifiedDeleted > io.ox/tasks//notifyNewModifiedDeleted

# Documents
modules/com.open-xchange.documents > io.ox/office//module
