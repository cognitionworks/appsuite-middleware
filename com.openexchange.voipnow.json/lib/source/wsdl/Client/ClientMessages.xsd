	<xs:element name="MoveClientsRequest">
		<xs:annotation>
			<xs:documentation>Move clients to a specific reseller account: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="resellerID" type="common:positiveInteger">
						<xs:annotation>
							<xs:documentation>New owner ID for client accounts </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="resellerIdentifier" type="common:string">
						<xs:annotation>
							<xs:documentation>New owner identifier for client accounts </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:choice>
					<xs:element name="chargingPlanID" type="common:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Charging plan ID associated with moved clients</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="chargingPlanIdentifier" type="common:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Charging plan identifier associated with moved clients</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:choice>
					<xs:element name="ID" type="common:positiveInteger" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Client IDs</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="identifier" type="common:string" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Client identifiers</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MoveClientsResponse" type="messagesInfo:MoveClientsResponseType">
		<xs:annotation>
			<xs:documentation>Move clients to a specific reseller account: response</xs:documentation>
		</xs:annotation>
	</xs:element>
</xs:schema>
