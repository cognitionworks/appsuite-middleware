<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://4psa.com/PBXMessages.xsd/2.5.1" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:data="http://4psa.com/PBXData.xsd/2.5.1" xmlns:common="http://4psa.com/Common.xsd/2.5.1"
	xmlns:messagesInfo="http://4psa.com/PBXMessagesInfo.xsd/2.5.1"
	targetNamespace="http://4psa.com/PBXMessages.xsd/2.5.1" elementFormDefault="qualified"
	id="PBXMessages">
	<xs:import namespace="http://4psa.com/PBXData.xsd/2.5.1" schemaLocation="PBXData.xsd"/>
	<xs:import namespace="http://4psa.com/PBXMessagesInfo.xsd/2.5.1"
		schemaLocation="PBXMessagesInfo.xsd"/>
	<xs:import namespace="http://4psa.com/Common.xsd/2.5.1" schemaLocation="../Common.xsd"/>
	<xs:element name="GetSystemAPISettingsRequest">
		<xs:annotation>
			<xs:documentation>Get SystemAPI settings: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="userID" type="common:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>User ID</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="userIdentifier" type="common:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>User identifier</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetSystemAPISettingsResponse">
		<xs:annotation>
			<xs:documentation>Get SystemAPI settings: response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="access">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="legacyAuth" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>Allow legacy authentication</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="status" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>CallAPI control:
										enabled/disabled</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetCallAPISettingsRequest">
		<xs:annotation>
			<xs:documentation>Get CallAPI settings: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element name="userID" type="common:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>User ID</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="userIdentifier" type="common:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>User identifier</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetCallAPISettingsResponse">
		<xs:annotation>
			<xs:documentation>Get CallAPI settings: response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="access">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="legacyAuth" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>Allow legacy authentication</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="status" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>CallAPI control:
										enabled/disabled</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="auth">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="password" type="common:password">
								<xs:annotation>
									<xs:documentation>CallAPI password</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="username" type="common:string">
								<xs:annotation>
									<xs:documentation>CallAPI username</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetRegionsRequest">
		<xs:annotation>
			<xs:documentation>Get all available regions for a country: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="code" type="common:string">
					<xs:annotation>
						<xs:documentation>Country code</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="region" type="common:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Region name to filter result</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetRegionsResponse">
		<xs:annotation>
			<xs:documentation>Get all available regions for a country: response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="region" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence minOccurs="0">
							<xs:element name="ID" type="common:positiveInteger" minOccurs="0"/>
							<xs:element name="name" type="common:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Region name</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="default" type="common:boolean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Default region for chosen
										country</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="notice" type="common:notice" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetTimezoneRequest">
		<xs:annotation>
			<xs:documentation>Get all available timezones: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="code" type="common:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Country code</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetTimezoneResponse">
		<xs:annotation>
			<xs:documentation>Get all available timezones: response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="timezone" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence minOccurs="0">
							<xs:element name="ID" type="common:positiveInteger" minOccurs="0"/>
							<xs:element name="name" type="common:string" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Timezone description</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="notice" type="common:notice" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="PingRequest" type="common:string">
		<xs:annotation>
			<xs:documentation>Ping 4PSA VoipNow server: request</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PingResponse" type="messagesInfo:PingResponseType">
		<xs:annotation>
			<xs:documentation>Ping 4PSA VoipNow server: response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EnrollRequest">
		<xs:annotation>
			<xs:documentation>Enroll 4PSA VoipNow in third-party applications:
				request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="scope" type="common:string">
					<xs:annotation>
						<xs:documentation>The enrollment scope. Scope:automation is reserved for
							4PSA VoipNow Automation </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="URL" type="common:domain">
					<xs:annotation>
						<xs:documentation>IP/URL of the infrastructure controller that enrolls
							(master host)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="masterAuth" type="common:string">
					<xs:annotation>
						<xs:documentation>Authentication key of the master</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="masterAuthType" type="common:string">
					<xs:annotation>
						<xs:documentation>Authentication type of the master</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="param" minOccurs="0" maxOccurs="64">
					<xs:annotation>
						<xs:documentation>Pairs of name/value parameters</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element name="name" type="common:string" minOccurs="0"/>
							<xs:element name="value" type="xs:string" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EnrollResponse" type="messagesInfo:EnrollmentResponseType">
		<xs:annotation>
			<xs:documentation>Enroll 4PSA VoipNow in third-party applications:
				response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WithdrawRequest">
		<xs:annotation>
			<xs:documentation>Withdraw 4PSA VoipNow in third-party applications:
				request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="enrollmentID" type="common:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Enrollment unique identifier</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="WithdrawResponse">
		<xs:annotation>
			<xs:documentation>Withdraw 4PSA VoipNow in third-party applications:
				response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="result" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Operation result</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="success"/>
							<xs:enumeration value="partial"/>
							<xs:enumeration value="failure"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="UpgradeVoipNowRequest" type="common:string">
		<xs:annotation>
			<xs:documentation>Upgrade 4PSA VoipNow to latest release: request</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UpgradeVoipNowResponse" type="messagesInfo:UpgradeVoipNowResponseType">
		<xs:annotation>
			<xs:documentation>Upgrade 4PSA VoipNow to latest release: response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UpgradeHistoryRequest" type="common:string">
		<xs:annotation>
			<xs:documentation>Upgrade 4PSA VoipNow history report: request</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="UpgradeHistoryResponse" type="messagesInfo:UpgradeHistoryResponseType">
		<xs:annotation>
			<xs:documentation>Upgrade 4PSA VoipNow history report: response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GetSchemaVersionsRequest" type="common:string">
		<xs:annotation>
			<xs:documentation>Get all available schema versions: request</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GetSchemaVersionsResponse" type="messagesInfo:GetSchemaVersionsResponseType">
		<xs:annotation>
			<xs:documentation>Get all available schema versions: response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GetInterfaceLangRequest" type="common:string">
		<xs:annotation>
			<xs:documentation>Get available interface languages: request</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GetInterfaceLangResponse" type="messagesInfo:GetInterfaceLangResponseType">
		<xs:annotation>
			<xs:documentation>Get available interface languages: response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GetPhoneLangRequest" type="common:string">
		<xs:annotation>
			<xs:documentation>Get the list with available phone languages:
				request</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GetPhoneLangResponse" type="messagesInfo:GetPhoneLangResponseType">
		<xs:annotation>
			<xs:documentation>Get the list with available phone languages:
				response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GetAdvertisingTemplatesRequest">
		<xs:annotation>
			<xs:documentation>Get user templates list: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:annotation>
					<xs:documentation>Either of</xs:documentation>
				</xs:annotation>
				<xs:element name="ID" type="common:positiveInteger" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Template ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="userLevel" type="common:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Possible values: 10 (reseller).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice>
						<xs:element name="userID" type="common:positiveInteger" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Template owner ID</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="userIdentifier" type="common:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Template owner identifier</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:element name="industryID" type="common:positiveInteger" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Industry ID that used the template.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetAdvertisingTemplatesResponse"
		type="messagesInfo:GetAdvertisingTemplatesResponseType">
		<xs:annotation>
			<xs:documentation>Get user templates list: response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="GetTemplatesRequest">
		<xs:annotation>
			<xs:documentation>Get user templates list: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:annotation>
					<xs:documentation>Either of</xs:documentation>
				</xs:annotation>
				<xs:element name="ID" type="common:positiveInteger" nillable="false" minOccurs="0">
					<xs:annotation>
						<xs:documentation>User template ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:sequence>
					<xs:element name="userLevel" type="common:integer" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Possible values: 10 (reseller), 50 (client), 100
								(extension).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="extensionType" default="term" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Filter by extension type</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="term"/>
								<xs:enumeration value="queue"/>
								<xs:enumeration value="ivr"/>
								<xs:enumeration value="voicecenter"/>
								<xs:enumeration value="conference"/>
								<xs:enumeration value="callback"/>
								<xs:enumeration value="callcard"/>
								<xs:enumeration value="intercom"/>
								<xs:enumeration value="queuecenter"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:choice>
						<xs:element name="userID" type="common:positiveInteger" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Template owner ID</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="userIdentifier" type="common:string" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Template owner identifier</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetTemplatesResponse" type="messagesInfo:GetTemplatesResponseType">
		<xs:annotation>
			<xs:documentation>Get user templates list: response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AddIndustryRequest">
		<xs:annotation>
			<xs:documentation>Add industry: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="name" type="common:string">
					<xs:annotation>
						<xs:documentation>Industry name</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="standardZoneCode" type="common:text">
					<xs:annotation>
						<xs:documentation>Standard zone code for minimized window</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="standardZoneHeight" type="common:positiveInteger" default="100">
					<xs:annotation>
						<xs:documentation>Standard zone height (in pixels); default value is 100 and
							can be set up to 200.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="extendedZoneCode" type="common:text" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Extended zone code for expanded advertising window. This
							code is optional.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="extendedZoneHeight" type="common:positiveInteger" default="450"
					minOccurs="0">
					<xs:annotation>
						<xs:documentation>Extended zone height (in pixels); default value is 450 an
							d can be set up to 800. This value is optional.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddIndustryResponse">
		<xs:annotation>
			<xs:documentation>Add industry: response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ID" type="common:positiveInteger" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Object ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="result" type="xs:string">
					<xs:annotation>
						<xs:documentation>Operation result</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="notice" type="common:notice" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EditIndustryRequest">
		<xs:annotation>
			<xs:documentation> Edit industry: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ID" type="common:positiveInteger">
					<xs:annotation>
						<xs:documentation>Object ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="name" type="common:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Industry name</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="standardZoneCode" type="common:text" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Standard zone code for minimized window</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="standardZoneHeight" type="common:positiveInteger" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Standard zone height (in pixels); this value can be set up
							to 200</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="extendedZoneCode" type="common:text" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Extended zone code for expanded advertising window.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="extendedZoneHeight" type="common:positiveInteger" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EditIndustryResponse">
		<xs:annotation>
			<xs:documentation>Edit industry: response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ID" type="common:positiveInteger" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Object ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="result">
					<xs:annotation>
						<xs:documentation>Operation result; value comes from the list {'succes',
							'failure'}</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="success"/>
							<xs:enumeration value="failure"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="notice" type="common:notice" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetIndustriesRequest">
		<xs:annotation>
			<xs:documentation>Get industries: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ID" type="common:positiveInteger" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Industry ID.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="filter" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Filter industries by name</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="GetIndustriesResponse" type="messagesInfo:GetIndustriesResponseType">
		<xs:annotation>
			<xs:documentation>Get industries: response</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="DelIndustriesRequest">
		<xs:annotation>
			<xs:documentation>Delete industries: request</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ID" type="common:positiveInteger" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Industry IDs to be deleted.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DelIndustriesResponse">
		<xs:annotation>
			<xs:documentation>DeleteIndustries: response</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="items" type="xs:positiveInteger" minOccurs="0">
