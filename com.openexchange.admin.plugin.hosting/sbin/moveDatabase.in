#!/bin/bash

OXFUNCTIONS=@oxfunctions@
OXCONFIG=@oxscriptconf@

test -f $OXFUNCTIONS || {
	echo "missing common shell functions file"
	exit 1
}

. $OXFUNCTIONS

test -f $OXCONFIG && . $OXCONFIG

ox_set_JAVA_BIN


test -z "$SERVERNAME"       && SERVERNAME=$HOSTNAME
test -z "$LIBPATH"          && LIBPATH=${PREFIX}jar


$JAVA_BIN -classpath $LIBPATH/ox_admindaemon.jar: \
	com.openexchange.admin.adminConsole.OXContext --command=move_database $*
